{
  "Routes": [
    {
      "DownstreamPathTemplate": "/Order/orders",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5006
        }
      ],
      "UpstreamPathTemplate": "/{your_route_template}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "RouteIsCaseSensitive": false,
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer",
        "AllowedScopes": []
      },
      "RateLimitOptions": {
        "ClientWhitelist": [],
        "EnableRateLimiting": false,
        "Period": "5s",
        "PeriodKey": "{your_service_name}_rate_limit_per_sec",
        "Limit": 100
      },
      "QoSOptions": {
        "ExceptionsAllowedBeforeBreaking": 50,
        "DurationOfBreak": 5000,
        "TimeoutValue": 30000
      }
    }
  ]
}